// model B is not declared

model A() = initially

model Main(simulator) = 
  initially
    x  =  create A() &
    mode  =  "Init" &
    y  =  0
  always
  simulator.timeStep + == 1 &
  simulator.endTime + == 1 &
  match mode with [
    "Init" ->
      if x.modelName == type(B) then
        y+ == 1
      else
        y+ == 2
      &
      mode + == "Idle"
    |"Idle" ->
  ]
 

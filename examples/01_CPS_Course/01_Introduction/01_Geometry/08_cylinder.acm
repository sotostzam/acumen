// Displaying a cylinder

model Main(simulator) = 
initially
  _3D = (Cylinder              // Type of 3D object
           center = (0,0,0)    // Center point
           radius = 0.1        // radius
           length = 4          // length
           color = yellow      // Color
           rotation = (0,0,0)) // Orientation




// Negation as Second Derivative Example
//
// Note:  Run with Semantics -> Enclosure

model Main(simulator) =
  initially 
    x    = 1 & 
    x'   = 0 & 
    mode = ""
  always
  match mode with [
    "" ->
      x' == -x
  ] &
  simulator.endTime+ == 1.0 &
  simulator.minSolverStep+ == 0.1

// Note:  No surprises.
